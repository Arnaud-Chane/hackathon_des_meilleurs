const [dataLike, setDataLike] = useState([]);
  useEffect(() => {
    const data = JSON.parse(localStorage.getItem("dataLike"));
    if (data) {
      setDataLike(data);
    }
  }, []);
  const handleRemoveItem = (id) => {
    const filteredData = dataLike.filter((item) => item.id !== id);
    localStorage.setItem("dataLike", JSON.stringify(filteredData));
    setDataLike(filteredData);
  };



