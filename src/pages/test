const [arraySpotifyImageUri, setArraySpotifyImageUri] = useState([]);
  useEffect(() => {
    const data = JSON.parse(localStorage.getItem("dataUser"));
    if (data) {
      setArraySpotifyImageUri(...data);
    }
  }, []);