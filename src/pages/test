const handleImageClick = (item) => {
    localStorage.setItem("dataUser", JSON.stringify([item]));
    const index = arraySpotifyImageUri.findIndex((obj) => obj === item);
    const index2 = arraySpotifyImage.findIndex((obj) => obj === item);
    console.info(item);
    if (index !== -1) {
      const correspondingPreview = arraySpotifyPreviewToObject[index];
      localStorage.setItem("music", JSON.stringify([correspondingPreview]));
      const correspondingNightClub = nightClubs[index];
      localStorage.setItem(
        "dataUser",
        JSON.stringify([correspondingNightClub])
      );
    } else if (index2 !== -1) {
      const correspondingPreview = arraySpotifyPreviewToObject[index2];
      localStorage.setItem("music", JSON.stringify([correspondingPreview]));
      const correspondingNightClub = nightClubs[index2];
      localStorage.setItem(
        "dataUser",
        JSON.stringify([correspondingNightClub])
      );
    }
  };




